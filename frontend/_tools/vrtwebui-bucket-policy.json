{
  "Version": "2012-10-17",
  "Id": "BeanstalkS3Copy",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:GetObject",
        "s3:GetObjectAcl",
        "s3:GetObjectRetention",
        "s3:GetObjectVersion",
        "s3:GetObjectVersionAcl"
      ],
      "Resource": [
        "arn:aws:s3:::vrtwebui",
        "arn:aws:s3:::vrtwebui/*"
      ]
    }
  ]
}
