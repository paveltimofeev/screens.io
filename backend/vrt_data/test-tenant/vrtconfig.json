{
  "id": "yandex",
  "viewports": [
    {
      "label": "Desktop HD",
      "width": 1920,
      "height": 1080
    },
    {
      "label": "Desktop",
      "width": 1600,
      "height": 900
    }
  ],
  "onBeforeScript": "",
  "onReadyScript": "",
  "scenarios": [
    {
      "label": "HomePage",
      "url": "http://localhost:4200/",
      "misMatchThreshold": "",
      "hideSelectors": [
        "#history-container"
      ]
    },
    {
      "label": "ConfigurationPage",
      "url": "http://localhost:4200/config/1",
      "misMatchThreshold": "",
      "hideSelectors": [
        "#history-container"
      ]
    },
    {
      "label": "404Page",
      "url": "http://localhost:4200/unknown-route",
      "misMatchThreshold": "",
      "hideSelectors": [
        "#history-container"
      ]
    }
  ],
  "report": [
    "CI",
    "json"
  ],
  "engine": "puppeteer",
  "engineOptions": {
    "args": [
      "--no-sandbox"
    ]
  },
  "asyncCaptureLimit": 5,
  "asyncCompareLimit": 50,
  "debug": false,
  "debugWindow": false
}
